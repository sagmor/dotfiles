#!/bin/sh

# Install YADR
sh -c "`curl -fsSL https://raw.github.com/skwp/dotfiles/master/install.sh`"

# Symlink Dotfiles changes
rmdir $HOME/.zsh.before
ln -s $HOME/.dotfiles/zsh/before $HOME/.zsh.before
rmdir $HOME/.zsh.after
ln -s $HOME/.dotfiles/zsh/after $HOME/.zsh.after
ln -s $HOME/.dotfiles/git/config $HOME/.gitconfig.user

# Install homebrew Cask
brew tap phinze/homebrew-cask
brew install brew-cask

brew install macvim --custom-icons
brew linkapps

#install apps
# adium arduino vlc transmit transmission spotify onepassword kindle iterm2 heroku-toolbelt google-hangouts google-chrome github evernote firefox dropbox
# brew cask install iterm2
# brew cask install spotify
# brew cask install spotify

#API_KEY="abcdef123456" brew cask install prey
